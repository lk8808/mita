/*手风琴样式调整*/
#RightAccordion .panel-title {
	padding-left: 10px;
}

/*菜单树样式调整*/
.tree-title {
	margin-left: 10px;
	font-weight: bold;
	color: #666;
}
.tree-node-selected .tree-title {
	color: #fff;
}
.tree-folder-open {
    background: none;
}

.accordion .accordion-header-selected {
	background: #F2F2F2;
}
.accordion .accordion-header-selected .panel-title {
	color: #777;
}

.fa-fw {
    padding: .2em .25em .15em;
    border-radius: .1em;
}


/*应用导航栏主面板样式*/
#appnavpanel
{
	height:60px;  
	border-top:0px solid #bebebe; 
	border-bottom:0px solid #bebebe; 
	border-right:0px solid #bebebe; 
	overflow: hidden;
	position: absolute; 
	left:450px;

}
/*应用导航栏左箭头样式*/
.navleftarrow
{
	background:url(../img/main//position-item-bg-leftarrow.png) no-repeat;
	_background:url(../img/main//position-item-bg-leftarrow.gif) no-repeat;
	background-position:center;
	height:50px;
	width:30px;
	display:block;
	float:left;
	cursor: pointer;
	visibility: hidden;
	margin-top: 3px;	
}
/* 导航栏右箭头样式 */
.navrightarrow
{
	background:url(../img/main//position-item-bg-rightarrow.png) no-repeat;
	 _background:url(../img/main//position-item-bg-rightarrow.gif) no-repeat;
	 background-position:center;
	height:50px;
	width:30px;
	display:block;
	float:left;
	cursor: pointer;
	visibility: hidden;
	margin-top: 3px;

}
/*导航栏样式*/
#appnavpanel div{
	height:100%;
	width:300px;
	position: relative; 
	float: left; 
	overflow: hidden;
}

#appnavpanel ul li {
	float:left;
	padding-right: 0px;
	padding-left: 0px;
	border-right: 0px solid #b8b8b8;
	text-align: center;
	cursor:pointer;
	height:60px;
	margin-right:10px;
	width:90px;
	_width:95px;
}

#appnavpanel ul li span{
	height:15px;
	cursor:pointer;
	display:block;
	_display:block;
	padding-top: 2px;
	padding-bottom: auto;
	padding-right: 5px;
	padding-left: 5px;
	font-size: 12px;
}

#appnavpanel ul li img{
	height:35px;
	width:35px;
	padding-right: 0px;
	padding-left: 0px;
	cursor:pointer;
	display:block;
	padding-top: 3px;
	padding-bottom: 0px;
	margin:0px auto auto auto;
}

#appnavpanel ul{
	position: absolute;
	top:0px;
	left:0px;
	height:60px;
	width:3000px;
	float:left;
	color:#FFFFFF;
}

.appHighLight{
	color:#EBFAEB;
	background-color: #0081c2;
	/* background:url(images/position-item-bg.gif); */
}

.appMenuActive{
	/* background:url(images/menu-item-current.gif); */
	color:#EBFAEB;
	background-color: #0081c2;
	font-weight: bolder;
} 


