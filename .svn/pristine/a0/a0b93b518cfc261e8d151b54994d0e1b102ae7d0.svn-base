html,body{
	margin: 0px;
	padding: 0px;
	background-color: fcfcfc;
	overflow: hidden;
}

*{margin:0px;padding:0px; font-family:微软雅黑;}

ul,li{ 
	padding:0;
	margin:0;
	list-style:none;
}

.main_header{
	background-color:#459df5 ;
	background-repeat: repeat-x;
	overflow: hidden;
	height: 60px;
	width: 100%;
	border:0px;
	border-bottom:1px solid #b8b8b8
}

.main_header .leftlogo{
	position: absolute;
 	top: 0px;
 	left: 0px;
 	z-index: 20;
 	overflow: hidden;
 	height: 50px !important;
	margin-top: 0px;
}

.main_header .leftlogo div{
	float:left;
}

.main_header .titlename{
	line-height: 40px;
	margin-left: 20px;
	margin-top: 8px;
	font-size: 26px;
	color:#FFFFFF;
}

.main_header .logoimg{
	float:left; 
	margin:8px 25px 0 25px; _margin-left:25px; _margin-right:25px; 
	width:180px; height:40px; 
	background:url(../img/main/company_logo.png) no-repeat; _background:url(../img/main/company_logo.png) no-repeat;
	background-size:180px 40px;
}

.main_header .rightop{
	position: absolute;
 	top: 20px;
 	right: 10px;
 	z-index: 100;
 	height: 30px !important;
 	color:#FFFFFF;
}

.main_header .rightop a{
	color:#FFFFFF;	
}


.layout-panel-west>.panel-header {
    padding: 6px;
    position: relative;
}

.menuicon{
	background:url('../img/main/icons/menu-icon.png') no-repeat center center;
}

.homeicon{
	background:url('../img/main/icons/home-index-icon.gif') no-repeat center center;
}

.main_footer {
	background:#459df5;
	height:24px;
	border:0px;
	border-top:1px solid #b8b8b8
}

.main_footer p{
	padding-top:4px;
	text-align:center;
	line-height:16px;
	color:#FFFFFF;
}

